{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "dot-notation": "warn",
    "no-var": "warn",
    "prefer-const": "warn",
    "no-duplicate-imports": "error"
  }
}
